{
    "name": "charlesportwoodii/yii2-api-rest-components",
    "description": "Restful API components for Yii framework 2",
    "keywords": ["yii2", "rest", "api", "cors", "auth"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/charlesportwoodii/yii2-api-rest-components/issues",
        "source": "https://github.com/charlesportwoodii/yii2-api-rest-components"
    },
    "authors": [{
        "name": "Charles R. Portwood II",
        "email": "charlesportwoodii@erianna.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.2",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-PDO": "*",
        "ext-Reflection": "*",
        "ext-redis": ">=3.1.0",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-pcre": "*",
        "ext-posix": "*",
        "ext-sodium": ">=2.0.0",
        "yiisoft/yii2": "~2.0.13",
        "yiisoft/yii2-redis": "~2.0.0",
        "yiisoft/yii2-twig": "~2.0.0",
        "yiisoft/yii2-smarty": "~2.0.0",
        "yiisoft/yii2-httpclient": "^2.0",
        "yii2mod/yii2-rbac": "*",
        "symfony/yaml": "^3.0",
        "spomky-labs/otphp": ">=8.2.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "christian-riesen/base32": "1.3.1",
        "monolog/monolog": "^1.23",
        "rpq/server": "dev-master"
    },
	"replace": {
		"bower-asset/jquery": "*",
		"bower-asset/inputmask": "*",
        "bower-asset/punycode": "*",
        "bower-asset/yii2-pjax": "*"
	},
	"conflict": {
		"bower-asset/jquery": "*",
		"bower-asset/inputmask": "*",
		"bower-asset/punycode": "*",
        "bower-asset/yii2-pjax": "*"
    },
    "autoload": {
        "psr-4": { "yrc\\": "" }
    }
}
