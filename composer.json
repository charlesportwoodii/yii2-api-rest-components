{
    "name": "charlesportwoodii/yii2-api-rest-components",
    "description": "Restful API components for Yii framework 2",
    "keywords": ["yii2", "rest", "api", "cors", "auth"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/charlesportwoodii/yii2-api-rest-components/issues",
        "source": "https://github.com/charlesportwoodii/yii2-api-rest-components"
    },
    "authors": [{
        "name": "Charles R. Portwood II",
        "email": "charlesportwoodii@erianna.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.2",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-PDO": "*",
        "ext-Reflection": "*",
        "ext-redis": "^3.1.2 || ^4.0.2",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-pcre": "*",
        "ext-posix": "*",
        "ext-sodium": ">=2.0.0",
        "symfony/yaml": "^4.1",
        "yidas/yii2-bower-asset": "~2.0.5",
        "yiisoft/yii2": "^2.0.15",
        "yiisoft/yii2-redis": "~2.0.7",
        "yiisoft/yii2-twig": "~2.2.0",
        "yiisoft/yii2-httpclient": "^2.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yii2mod/yii2-rbac": "*",
        "spomky-labs/otphp": ">=8.2.0",
        "christian-riesen/base32": "1.3.1",
        "rpq/server": "^0.1.1",
        "charlesportwoodii/ncryptf": "^0.1.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2",
        "yiisoft/yii2-coding-standards": "^2.0"
    },
    "autoload": {
        "psr-4": { "yrc\\": "" }
    }
}
